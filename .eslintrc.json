{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": "off",
        "no-use-before-define": "off",
        "react/style-prop-object": "off",
        "import/prefer-default-export" : "off",
        "global-require": "off"
    }
}
